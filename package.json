    "build": "rollup -c && cp -f src/index.d.ts dist/"
  },
  "author": "Dmitry Dudin <dima@nuware.ru>",
  "homepage": "https://github.com/nuware/invariant#readme",
  "bugs": {
    "url": "https://github.com/nuware/invariant/issues"
  },
  "repository": "github:nuware/invariant",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.16.0",
    "onchange": "^5.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {}
