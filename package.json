{
  "name": "@nuware/invariant",
  "version": "1.0.1",
  "description": "Simple invariant function",
  "main": "dist/invariant.cjs.js",
  "module": "dist/invariant.esm.js",
  "browser": "dist/invariant.umd.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.js",
